{
  "name": "multiple-scss-generation",
  "private": true,
  "scripts": {
    "sass": "node-sass --output-style expanded --source-map true --source-map-contents src/ --output build/",
    "postcss": "postcss build/*.css --ext post.css --map --dir build/"
  },
  "author": "Masataka Yakura",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3"
  }
}
